{
  "name": "ce-decorators",
  "version": "1.3.0",
  "description": "Custom Element decorators for typescript",
  "module": "lib/index.js",
  "main": "umd/bundle.min.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "rimraf ./reports && npm run test:all",
    "test:all": "rimraf ./reports && karma start --single-run true --watch false karma.conf.js",
    "test:all-auto": "karma start --auto-run karma.conf.js",
    "lint": "tslint --project ./",
    "build": "rimraf ./lib ./umd ./test && tsc --project ./tsconfig.dist.json && webpack",
    "serve": "webpack-dev-server --env.devserver"
  },
  "author": "Mathis Zeiher",
  "keywords": [
    "custom elements",
    "web components",
    "typescript",
    "decorators",
    "cev1",
    "lit-html",
    "ce"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mzeiher/ce-decorators.git"
  },
  "homepage": "https://github.com/mzeiher/ce-decorators.git",
  "license": "ISC",
  "peerDependencies": {
    "lit-html": "^0.13.0"
  },
  "devDependencies": {
    "@babel/core": "~7.1.2",
    "@babel/plugin-proposal-class-properties": "~7.1.0",
    "@babel/plugin-proposal-decorators": "~7.1.2",
    "@babel/polyfill": "~7.0.0",
    "@babel/preset-env": "~7.1.0",
    "@babel/preset-typescript": "~7.1.0",
    "@types/chai": "~4.1.6",
    "@types/jasmine": "~2.8.8",
    "@webcomponents/webcomponentsjs": "2.1.3",
    "babel-loader": "~8.0.4",
    "babel-plugin-istanbul": "~5.1.0",
    "corejs": "~1.0.0",
    "jasmine-core": "~3.2.1",
    "jsdom": "^12.2.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-detect-browsers": "~2.3.3",
    "karma-edge-launcher": "~0.4.2",
    "karma-firefox-launcher": "~1.1.0",
    "karma-ie-launcher": "~1.0.0",
    "karma-jasmine": "~1.1.2",
    "karma-jsdom-launcher": "~6.1.3",
    "karma-junit-reporter": "~1.2.0",
    "karma-mocha-reporter": "~2.2.5",
    "karma-source-map-support": "~1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "lit-html": "~0.13.0",
    "ts-loader": "~5.2.2",
    "tslint": "~5.11.0",
    "tslint-microsoft-contrib": "~5.2.1",
    "typescript": "~3.1.3",
    "webpack": "~4.20.2",
    "webpack-cli": "~3.1.2",
    "webpack-dev-server": "~3.1.9",
    "webpack-merge": "~4.1.4"
  }
}
