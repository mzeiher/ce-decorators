{
  "name": "ce-decorators",
  "version": "1.0.2",
  "description": "Custom Element decorators for typescript",
  "module": "es6/index.js",
  "browser": "es5/index.js",
  "main": "umd/bundle.min.js",
  "types": "es6/index.d.ts",
  "scripts": {
    "test": "npm run test:all",
    "test:all": "karma start --single-run --browser Chrome,Edge,IE karma.conf.js",
    "test:all-auto": "karma start --browser --auto-run Chrome,Edge,IE karma.conf.js",
    "test:chrome": "karma start --single-run --browser Chrome karma.conf.js",
    "build": "tsc --project ./tsconfig.es6.json && tsc --project ./tsconfig.es5.json && webpack"
  },
  "author": "Mathis Zeiher",
  "keywords": ["custom elements", "web components", "typescript", "decorators", "cev1", "lit-html", "ce"],
  "repository": {
    "type" : "git",
    "url" : "https://github.com/mzeiher/ce-decorators.git"
  },
  "homepage": "https://github.com/mzeiher/ce-decorators.git",
  "license": "ISC",
  "dependencies": {
    "lit-html": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/chai": "^4.1.4",
    "@types/jasmine": "^2.8.8",
    "@webcomponents/webcomponentsjs": "^2.1.0",
    "babel-loader": "^8.0.2",
    "corejs": "^1.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.2.1",
    "jsdom": "^12.0.0",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-jsdom-launcher": "^6.1.3",
    "karma-mocha-reporter": "^2.2.5",
    "karma-source-map-support": "^1.3.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.0",
    "typescript": "^3.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  }
}
